@model List<IdentityServerBasicExample.Models.ClaimViewModel>;
@{
    var index = 0;
}
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Type</th>
      <th scope="col">Value</th>
    </tr>
  </thead>
  <tbody>
      @foreach (var item in Model)
        {
            index++;
            <tr>
              <th scope="row">@index.ToString()</th>
              <td>@item.ClaimName</td>
              <td>@item.ClaimValue</td>
            </tr>
      }
  </tbody>
</table>